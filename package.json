{
  "name": "ts-express-angular2-node-boilerplate",
  "version": "1.0.0",
  "description": "Light-weight startup boilerplate for angular2 with typescript and expressjs",
  "repository": "https://github.com/tngan/ts-express-angular2-node-boilerplate.git",
  "author": "tngan",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run tsc:w\" \"npm run server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "server": "nodemon ./server/bin/www",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "ejs": "^2.4.2",
    "es6-shim": "^0.35.1",
    "eslint": "^3.1.1",
    "express": "^4.13.4",
    "express-session": "^1.11.3",
    "reflect-metadata": "0.1.2",
    "systemjs": "0.19.27"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "core-js": "^2.4.0",
    "rxjs": "5.0.0-beta.6",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "zone.js": "^0.6.12"
  }
}
